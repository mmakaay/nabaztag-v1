include core/leds/off

music_file TARDIS midi/doctorwho.mid

define $vol  R0
define $file R0

@Main
    BSR @Leds.Off()

    # Set volume
    LD $vol 255
    MVOL $vol
    LD $vol 150
    VOL $vol

    # Start MIDI playback
    LD $file, music_file:TARDIS
    MIDIPLAY $file

@end
    WAIT 10
    BRA @end
