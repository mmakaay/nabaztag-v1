include core/leds/off
include core/leds/twinkle

music_file LIEF midi/liefkleinkonijntje.mid

@Main
    LD R0 255
    MVOL R0
    LD R0 150
    VOL R0

    LD R0, music_file:LIEF
    MIDIPLAY R0

@wait_for_end_of_midi
    BSR @Leds.Twinkle()
    MUSIC R0
    TST R0
    BNE @wait_for_end_of_midi
    BSR @Leds.Off()

@end
    WAIT 10
    BRA @end
