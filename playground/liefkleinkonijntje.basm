include leds

music_file LIEF liefkleinkonijntje.mid

@Main
    LD R0 255
    MVOL R0
    LD R0 150
    VOL R0

    LD R0, music_file:LIEF
    MIDIPLAY R0

@wait_for_end_of_midi
    BSR @TwinkleLeds()
    MUSIC R0
    TST R0
    BNE @wait_for_end_of_midi
    BSR @TurnAllLedsOff()

@end
    WAIT 10
    BRA @end
