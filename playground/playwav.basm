include core/sound/off

music_file V1 audio/music_0.vox
music_file V2 audio/music_1.vox

define $vol      R0
define $wav_file R0

@Main
    BSR @Sound.Off()

    LD $vol, 255
    MVOL $vol
    LD $vol, 150
    VOL $vol

    LD $wav_file, music_file:V1
    WAVPLAY $wav_file

@end
    WAIT 20
    BRA @end
